   <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
      <Button fx:id="utcButton" disable="true" mnemonicParsing="false" onAction="#getDate" prefWidth="125.0" text="Get UTC Date/Time" />
