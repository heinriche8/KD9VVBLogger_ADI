      </VBox.margin></TextField>
   <TextField fx:id="opBox" editable="false" promptText="Operator">
      <VBox.margin>
         <Insets left="10.0" right="10.0" />
      </VBox.margin></TextField>
   <TextField fx:id="dateBox" editable="false" promptText="UTC Date">
      <VBox.margin>
         <Insets left="10.0" right="10.0" />
      </VBox.margin></TextField>
   <TextField fx:id="timeBox" editable="false" promptText="UTC Time">
      <VBox.margin>
         <Insets left="10.0" right="10.0" />
      </VBox.margin></TextField>
   <ChoiceBox fx:id="modeBox" disable="true" prefWidth="150.0">
      <VBox.margin>
         <Insets left="10.0" right="10.0" />
